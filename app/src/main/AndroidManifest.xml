<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.syncdating">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SyncDating">
        <activity android:name=".RegistrationPage2"></activity>
        <activity android:name=".ForgetPassword.ForgetPasswordSuccessMessage" />
        <activity android:name=".ForgetPassword.SetNewPassword" />
        <activity android:name=".ForgetPassword.MakeSelection" />
        <activity android:name=".ForgetPassword.ForgetPassword" />
        <activity android:name=".LoginAndRegistration.Login1" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginAndRegistration.LoginActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".Matches.MatchesActivity" />
        <activity android:name=".Chat.ChatActivity" />
    </application>

</manifest>